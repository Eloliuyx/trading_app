    "release:major": "npm version major -m \"chore(release): %s\" && git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "lightweight-charts": "^4.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.7"
  },
  "version": "1.3.0"
}
