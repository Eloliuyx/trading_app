{
  "name": "trading-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "vite --config frontend/vite.config.mts",
    "build": "vite build --config frontend/vite.config.mts",
    "preview": "vite preview --config frontend/vite.config.mts --port 5173",
    "lint": "eslint --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "release:patch": "npm version patch -m \"chore(release): %s\" && git push && git push --tags",
    "release:minor": "npm version minor -m \"chore(release): %s\" && git push && git push --tags",
    "release:major": "npm version major -m \"chore(release): %s\" && git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "lightweight-charts": "^4.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.5",
    "zustand": "^4.5.7"
  },
  "version": "1.3.0"
}
