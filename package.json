{
  "name": "trading-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "vite --config frontend/vite.config.mts",
    "build": "vite build --config frontend/vite.config.mts",
    "preview": "vite preview --config frontend/vite.config.mts --port 5173",
    "release:patch": "npm version patch -m \"chore(release): %s\" && git push && git push --tags",
